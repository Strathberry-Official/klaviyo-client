{
  "name": "@strathberry/klaviyo-client",
  "version": "1.0.3",
  "description": "(Fork) Klaviyo API client for Node.js and browser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/matthewerskine/klaviyo-client",
  "author": "Matthew Erskine <matthew@erskine.com>, Mikhail Monchak <mikhail.monchak.work@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Klaviyo",
    "klaviyo.com"
  ],
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "pub": "npm version patch && npm publish"
  },
  "devDependencies": {
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint",
      "pre-push": "npm run build"
    }
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "js-base64": "^3.6.0"
  }
}
